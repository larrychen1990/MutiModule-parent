<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
		
	<bean id="demoMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">  
        <property name="sqlSessionFactory" ref="sqlSessionFactory" />  
        <property name="mapperInterface" value="com.alexgaoyh.MutiModule.persist.demo.DemoMapper" />  
	</bean>
	
	<bean id="sysmanUserMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">  
        <property name="sqlSessionFactory" ref="sqlSessionFactory" />  
        <property name="mapperInterface" value="com.alexgaoyh.MutiModule.persist.sysman.SysmanUserMapper" />  
	</bean>
	
	<bean id="sysmanRoleMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">  
        <property name="sqlSessionFactory" ref="sqlSessionFactory" />  
        <property name="mapperInterface" value="com.alexgaoyh.MutiModule.persist.sysman.SysmanRoleMapper" />  
	</bean>
	
	<bean id="sysmanResourceMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">  
        <property name="sqlSessionFactory" ref="sqlSessionFactory" />  
        <property name="mapperInterface" value="com.alexgaoyh.MutiModule.persist.sysman.SysmanResourceMapper" />  
	</bean>
	
	<bean id="sysmanUserRoleRelMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">  
        <property name="sqlSessionFactory" ref="sqlSessionFactory" />  
        <property name="mapperInterface" value="com.alexgaoyh.MutiModule.persist.sysman.SysmanUserRoleRelMapper" />  
	</bean>
	
	<bean id="sysmanRoleResourceRelMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">  
        <property name="sqlSessionFactory" ref="sqlSessionFactory" />  
        <property name="mapperInterface" value="com.alexgaoyh.MutiModule.persist.sysman.SysmanRoleResourceRelMapper" />  
	</bean>
				
</beans>